
class Solution {
    public String reverseWords(String s) {
       // s.trim();
        String str[] = s.trim().split("[ ]+");
        String ans = "";
        for(int i = str.length-1;i>=0;i--) {
            ans+= str[i];
           if(i!=0) ans+=" ";
        }
        System.out.println(Arrays.toString(str));
        return ans;
    }
}